{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "start": {
      "persistent": true
    },
    "clean": {
      "dependsOn": [
        "^clean"
      ]
    },
    "clean:all": {
      "dependsOn": [
        "^clean:all"
      ]
    },
    "format": {
      "dependsOn": [
        "^format"
      ]
    }
  }
}
